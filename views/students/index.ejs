<% include ../partials/boilerplate %>
<h1>Hello</h1>
<% students.forEach(student => { %>
<div class="student">

    <h4>
        <a href="<%= `/student/${student.id}` %>">
            <p><%= student.id %></p>
        </a>
            <p><%= student.student_name %></p>
            <p><%= student.email %></p>
            <p><%= student.gender %></p>
            <p><%= student.phone_number %></p>

    </h4>

</div>
<% }) %>

<% include ../partials/end %>
